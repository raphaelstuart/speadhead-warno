//-------------------------------------------------------------------------
//------------------------- MISSIONS D'ATTAQUE ----------------------------
//-------------------------------------------------------------------------
// Parametres TMacroActionDescriptor_Common
//-------------------------------------------------------------------------
// GenerationType :
// NumberLaunchBasedOnGenerationType :
// ParametresDeMission : Transport, Airstrike, Support, ArtilleryStrike
// PoolModelList : liste des unites demandees
// DisableReinforcement : si on recoit des renforts en cours d'attaque ou pas
// IgnoreSafePath : Le pathfind de la mission cherche ou non à esquiver la menace
// TuerLaMissionEnFinDeVieAuLieuDeLaRelancer : (bool)
// LaunchASAP_RatioMin : permet de lancer l'attaque sans attendre toutes les unites requises à combiner avec ProdProperties = [TProdProperty_LaunchAsSoonAsPossible]
// MissionOmnisciente : Pas utilise dans Normandie (systeme de connaissance de zone)
//-------------------------------------------------------------------------
// Parametres TMacroActionDescriptor_Attack_Specific
//-------------------------------------------------------------------------
// RatioMinimumPourAttaquer : (float) le ratio qu'on doit posséder pour pouvoir déclencher l'attaque
// ForceZoneAttaqueName : (string)
// InterdireRequisition : (bool) si on utilise le systeme de Defense de base d'AoA
//-------------------------------------------------------------------------
// Parametres supplémentaire TMacroActionDescriptor_CorridorAttack_Specific
//-------------------------------------------------------------------------
// ForcedCorridor : (format special ex:[1]) permet de forcer l'attaque sur un couloir.
//

//-------------------------------------------------------------------------
// Settings
//-------------------------------------------------------------------------
Default_Support_Request_Attack_Settings is
[
    TAirStrikeSettingHolder(Setting = ~/BasicAirstrikeSupport),
    TSupportSettingHolder(SettingList = [Renfort_TransportsAbandonnes]),
    TArtilleryAmountSettingHolder(ArtilleryAmount = 2),
]
//-------------------------------------------------------------------------
Blitzkrieg_Attack_Settings is
[
    TBlitzkriegSettingHolder(RatioFactorToDisableSelfPreservation = 2.0   MinimumCostForSelfPreservation = 100),
]
//-------------------------------------------------------------------------
AvoidFillerCorridors_Attack_Settings is
[
    TAvoidCorridorTagsSettingHolder(AvoidedCorridorTags = ["Filler"]),
]
//-------------------------------------------------------------------------
// Attaque d'objectifs
//-------------------------------------------------------------------------
private template Template_Objective_Attack
[
    MinimumResourceForLaunch = MAP[],
    AdditionnalParameter = [],
]
is TMacroActionDescriptor_ObjectiveAttack_Specific
(
    ParametresDeMission =
    ~/Default_Support_Request_Attack_Settings
    +
    ~/Blitzkrieg_Attack_Settings
    +
    [
        TGenerationSettingHolder(GenerationType = TGenerationType_FixedSize NumberLaunchBasedOnGenerationType = 1),
        TMacroActionProductionSettingHolder(ProdProperties = [TProdProperty_LaunchAsSoonAsPossible] PoolModelList = [Pool_Objective_Attack] LaunchASAP_RatioMin = 0.01),
        TAttackRatioSettingHolder(RatioMinimumPourAttaquer = 0.825),
        TAttackRatioSettingHolder(DifficultyList = [IADifficulty/TresFacile] RatioMinimumPourAttaquer = 100.0),
        TMacroActionLifetimeSettingHolder(MinimumResourceForLaunch = <MinimumResourceForLaunch>),
    ]
    +
    <AdditionnalParameter>
)
//-------------------------------------------------------------------------
export Attack_Objectives is Template_Objective_Attack
(
)
//-------------------------------------------------------------------------
export Attack_Objectives_Autogen is Template_Objective_Attack
(
    MinimumResourceForLaunch = ~/MinimumResourceForLaunch_Stage_1
)

//-------------------------------------------------------------------------
export Attack_Objectives_Allied_Autogen is Template_Objective_Attack
(
    MinimumResourceForLaunch = ~/MinimumResourceForLaunch_Stage_1
    AdditionnalParameter =
    [
        TObjectiveRequiresEnnemiesPossessionSettingHolder(),
        TObjectiveAttackSetting(ZoneSources = [EMapSlicingZoneSource/ScenarioIAStrat, EMapSlicingZoneSource/ScenarioCommand] PriorizeObjectiveNotInMyCorridor = true),
    ]
)

//-------------------------------------------------------------------------
// COULOIR
//-------------------------------------------------------------------------
private template Template_Main_Attack
[
    MinimumResourceForLaunch = MAP[],
    AdditionnalParameter = [],
    SupportingStatus = ESupportingStatus/NeedSupport,
    PoolModel_Generic = ~/Pool_Main_Attack,
    PoolModel_Easy = ~/Pool_Main_Attack_Easy,
]
is TMacroActionDescriptor_CorridorAttack_Specific
(
    ParametresDeMission =
    ~/Default_Support_Request_Attack_Settings
    +
    ~/Blitzkrieg_Attack_Settings
    +
    ~/AvoidFillerCorridors_Attack_Settings
    +
    [
        TGenerationSettingHolder(GenerationType = TGenerationType_FixedSize NumberLaunchBasedOnGenerationType = 1),
        TCoordinationSettingHolder(CoordinationType = <SupportingStatus>),

        TMacroActionProductionSettingHolder(ProdProperties = [TProdProperty_LaunchAsSoonAsPossible] PoolModelList = [<PoolModel_Generic>]
            TaggedPoolModelList = [(["AttackMain"], [<PoolModel_Generic>]),] PrioritizeCorridorsForTaggedPoolModels = true LaunchASAP_RatioMin = 0.01),
        TMacroActionProductionSettingHolder(DifficultyList = [IADifficulty/Facile] ProdProperties = [TProdProperty_LaunchAsSoonAsPossible] PoolModelList = [<PoolModel_Easy>] TaggedPoolModelList = [(["AttackMain"], [<PoolModel_Easy>]),] PrioritizeCorridorsForTaggedPoolModels = true LaunchASAP_RatioMin = 0.01),

        TAttackRatioSettingHolder(RatioMinimumPourAttaquer = 0.875 RatioPourAttaquerPerduAChaqueTick = 0.00084), // Perte de 0.1 de ratio toutes les 2 minutes
        TMacroActionLifetimeSettingHolder(MinimumResourceForLaunch = <MinimumResourceForLaunch>),
        TCorridorPlacementSettingHolder(StayInCorridorIfp = true MargeDeRegroupementDepuisLaLigneDeFront = 1 ActiveComportementEnBoutDeCouloir = true),
    ]
    +
    <AdditionnalParameter>
)
